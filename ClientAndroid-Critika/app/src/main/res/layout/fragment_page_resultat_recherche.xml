<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout_resultat_recherche"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PageResultatRecherche">

    <ImageButton
        android:id="@+id/bouton_retour_resultat_recherche"
        android:background="?selectableItemBackgroundBorderless"
        android:scaleType="fitXY"
        android:layout_width="123dp"
        android:layout_height="115dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/logocriti"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/bouton_deconnection_resultat_recherche"
        android:background="?selectableItemBackgroundBorderless"
        android:scaleType="fitXY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/bouton_deconnection"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/fond_gris_resultat_recherche"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/rectangle_noir_3_"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/fond_rouge_resultat_recherche"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/rectangle_rouge_3_"
        app:layout_constraintBottom_toBottomOf="@+id/fond_gris_resultat_recherche"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/bouton_menu_resultat_recherche"
        android:background="?selectableItemBackgroundBorderless"
        android:scaleType="fitXY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:src="@drawable/home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/fond_rouge_resultat_recherche" />

    <ImageButton
        android:id="@+id/bouton_recherche_resultat_recherche"
        android:background="?selectableItemBackgroundBorderless"
        android:scaleType="fitXY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:src="@drawable/search"
        app:layout_constraintBottom_toBottomOf="@+id/fond_gris_resultat_recherche"
        app:layout_constraintEnd_toStartOf="@+id/bouton_compte_resultat_recherche"
        app:layout_constraintStart_toEndOf="@+id/bouton_menu_resultat_recherche"
        app:layout_constraintTop_toTopOf="@+id/fond_rouge_resultat_recherche" />

    <ImageButton
        android:id="@+id/bouton_compte_resultat_recherche"
        android:background="?selectableItemBackgroundBorderless"
        android:scaleType="fitXY"
        android:layout_width="93dp"
        android:layout_height="90dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="30dp"
        android:src="@drawable/user_menu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/fond_rouge_resultat_recherche"
        app:layout_constraintTop_toTopOf="@+id/fond_rouge_resultat_recherche" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycler_view_resultat_recherche"
        android:layout_width="409dp"
        android:layout_height="437dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/fond_gris_resultat_recherche"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bouton_retour_resultat_recherche" />
</androidx.constraintlayout.widget.ConstraintLayout>