<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout_changement_mot_passe"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ChangementMotPasse" >

    <ImageButton
        android:id="@+id/bouton_retour_menu_mot_passe"
        android:layout_width="123dp"
        android:layout_height="115dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:background="?selectableItemBackgroundBorderless"
        android:scaleType="fitXY"
        android:src="@drawable/logocriti"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/bouton_deconnection_menu_mot_passe"
        android:background="?selectableItemBackgroundBorderless"
        android:scaleType="fitXY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/bouton_deconnection"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/fond_gris_menu_mot_passe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/rectangle_noir_3_"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/fond_rouge_menu_mot_passe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/rectangle_rouge_3_"
        app:layout_constraintBottom_toBottomOf="@+id/fond_gris_menu_mot_passe"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/bouton_menu_mot_passe"
        android:background="?selectableItemBackgroundBorderless"
        android:scaleType="fitXY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:src="@drawable/home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/fond_rouge_menu_mot_passe" />

    <ImageButton
        android:id="@+id/bouton_recherche_menu_mot_passe"
        android:background="?selectableItemBackgroundBorderless"
        android:scaleType="fitXY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:src="@drawable/search"
        app:layout_constraintBottom_toBottomOf="@+id/fond_gris_menu_mot_passe"
        app:layout_constraintEnd_toStartOf="@+id/bouton_compte_mot_passe"
        app:layout_constraintStart_toEndOf="@+id/bouton_menu_mot_passe"
        app:layout_constraintTop_toTopOf="@+id/fond_rouge_menu_mot_passe" />

    <ImageButton
        android:id="@+id/bouton_compte_mot_passe"
        android:background="?selectableItemBackgroundBorderless"
        android:scaleType="fitXY"
        android:layout_width="93dp"
        android:layout_height="90dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="30dp"
        android:src="@drawable/user_menu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/fond_rouge_menu_mot_passe"
        app:layout_constraintTop_toTopOf="@+id/fond_rouge_menu_mot_passe" />

    <ImageButton
        android:id="@+id/boutou_sauvegarde_mot_passe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:background="?selectableItemBackgroundBorderless"
        android:scaleType="fitXY"
        android:src="@drawable/bouton_inscription__rouge"
        app:layout_constraintBottom_toTopOf="@+id/fond_gris_menu_mot_passe"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="411dp"
        android:layout_height="41dp"
        android:layout_marginBottom="50dp"
        android:text="Changez votre mot de passe"
        android:textAlignment="center"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/conteneur_changement_Mot_Passe"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/conteneur_changement_Mot_Passe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:src="@drawable/mot_de_passe_2_"
        app:layout_constraintBottom_toTopOf="@+id/conteneur_changement_Mot_Passe_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/conteneur_changement_Mot_Passe_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:src="@drawable/mot_de_passe_2_"
        app:layout_constraintBottom_toTopOf="@+id/boutou_sauvegarde_mot_passe"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/saisie_changement_mot_passe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Nouveau mot de passe"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="@+id/conteneur_changement_Mot_Passe"
        app:layout_constraintEnd_toEndOf="@+id/conteneur_changement_Mot_Passe"
        app:layout_constraintStart_toStartOf="@+id/conteneur_changement_Mot_Passe"
        app:layout_constraintTop_toTopOf="@+id/conteneur_changement_Mot_Passe" />

    <EditText
        android:id="@+id/saisie_changement_mot_passe_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Réécrivez le"
        android:inputType="textPassword"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/conteneur_changement_Mot_Passe_2"
        app:layout_constraintEnd_toEndOf="@+id/conteneur_changement_Mot_Passe_2"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="@+id/conteneur_changement_Mot_Passe_2"
        app:layout_constraintTop_toTopOf="@+id/conteneur_changement_Mot_Passe_2" />

</androidx.constraintlayout.widget.ConstraintLayout>