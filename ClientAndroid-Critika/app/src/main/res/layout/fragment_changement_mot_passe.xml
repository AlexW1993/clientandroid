<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Présentation.vue.VuePageJeu">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?selectableItemBackgroundBorderless"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <include
            android:id="@+id/toolbar_haut"
            layout="@layout/barre_menu_haut" />

    </com.google.android.material.appbar.AppBarLayout>

    <ImageButton
        android:id="@+id/boutou_sauvegarde_mot_passe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:background="?selectableItemBackgroundBorderless"
        android:scaleType="fitXY"
        android:src="@drawable/bouton_inscription__rouge"
        app:layout_constraintBottom_toTopOf="@+id/appBarLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="411dp"
        android:layout_height="41dp"
        android:layout_marginBottom="50dp"
        android:text="Changez votre mot de passe"
        android:textAlignment="center"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/conteneur_changement_Mot_Passe"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/conteneur_changement_Mot_Passe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:src="@drawable/mot_de_passe_2_"
        app:layout_constraintBottom_toTopOf="@+id/conteneur_changement_Mot_Passe_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/conteneur_changement_Mot_Passe_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:src="@drawable/mot_de_passe_2_"
        app:layout_constraintBottom_toTopOf="@+id/boutou_sauvegarde_mot_passe"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/saisie_changement_mot_passe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Nouveau mot de passe"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="@+id/conteneur_changement_Mot_Passe"
        app:layout_constraintEnd_toEndOf="@+id/conteneur_changement_Mot_Passe"
        app:layout_constraintStart_toStartOf="@+id/conteneur_changement_Mot_Passe"
        app:layout_constraintTop_toTopOf="@+id/conteneur_changement_Mot_Passe" />

    <EditText
        android:id="@+id/saisie_changement_mot_passe_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Réécrivez le"
        android:inputType="textPassword"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/conteneur_changement_Mot_Passe_2"
        app:layout_constraintEnd_toEndOf="@+id/conteneur_changement_Mot_Passe_2"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="@+id/conteneur_changement_Mot_Passe_2"
        app:layout_constraintTop_toTopOf="@+id/conteneur_changement_Mot_Passe_2" />
    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?selectableItemBackgroundBorderless"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <include
            android:id="@+id/toolbar_bas"
            layout="@layout/barre_menu_bas" />

    </com.google.android.material.appbar.AppBarLayout>
</androidx.constraintlayout.widget.ConstraintLayout>